#include <bits/stdc++.h> 
long long maxSubarraySum(int arr[], int n)
{
    long long int sum = 0;
    
    long long int ans = 0;
    
    for(int i = 0; i<n; i++)
    {
        sum += arr[i];
        if(sum > ans)
            ans = sum;
        else if(sum < 0)
            sum = 0;
    }
    return ans;
}
